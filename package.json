{
    "name": "cypress-selector-shorthand",
    "version": "0.0.3",
    "description": "Cypress plugin to reduce boilerplate for control selectors",
    "type": "module",
    "main": "./dist/navigationGenerator.js",
    "exports": {
        ".": "./dist/navigationGenerator.js",
        "./install": "./dist/install.js",
        "./generator": "./dist/schemaGenerator/interfaceGenerator.js"
    },
    "bin": {
        "cypress-selector-shorthand": "./dist/bin/cyGetShorthand.js"
    },
    "files": [
        "generator.js",
        "index.js",
        "install.js",
        "dist/bin/cyGetShorthand.js",
        "dist/install.js",
        "dist/navigationGenerator.js",
        "dist/schemaGenerator/interfaceGenerator.js",
        "dist/lib/cypress-selector-shorthand.d.ts"
    ],
    "types": "./dist/lib/cypress-selector-shorthand.d.ts",
    "scripts": {
        "build": "webpack --config webpack.config.cjs && tsc -p tsconfig.node.json",
        "precheck": "yarpm run madge",
        "check": "lint-staged",
        "check:all": "yarpm run precheck && yarpm run lint:all && yarpm run stop-only && yarpm run test",
        "clean": "rm -f cypress/todoInterface.ts && rm -rf dist",
        "cypress": "cypress open --config-file cypress.config.cts --e2e -b chrome",
        "dedupe": "jscpd",
        "dedupe:strict": "jscpd --mode strict --min-lines 1",
        "lint": "eslint --cache",
        "lint:fix": "yarpm run lint -- --fix",
        "lint:all": "yarpm run lint -- ./lib ./bin ./schemaGenerator generator.js index.js install.js README.md ./cypress cypress.config.cts webpack.config.cjs .eslintrc.cjs",
        "lint:all:fix": "yarpm run lint:all -- --fix",
        "madge": "madge --circular ./lib ./bin ./schemaGenerator ./cypress",
        "prepare": "husky install",
        "stop-only": "stop-only --folder cypress",
        "pretest": "yarpm run build && node ./dist/bin/cyGetShorthand.js -i ./cypress/todoSchema.json -o ./cypress/todoInterface.ts -n TodoApp",
        "test": "cypress run --config-file cypress.config.cts e2e/todo.cy.ts"
    },
    "lint-staged": {
        "*.(md|js|ts|jsx|tsx|cjs|cts|mjs|mts)": [
            "yarpm run stop-only -- --file",
            "yarpm run lint -- --fix",
            "yarpm run test"
        ]
    },
    "author": "Kinland",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kinland/cypress-selector-shorthand.git"
    },
    "keywords": [
        "cypress",
        "selector",
        "test"
    ],
    "license": "GPL-3.0",
    "dependencies": {
        "@commander-js/extra-typings": "^11.0.0",
        "commander": "^11.0.0",
        "cypress": "^12.17.2",
        "quicktype": "^23.0.59",
        "quicktype-core": "^23.0.59"
    },
    "devDependencies": {
        "@cypress/webpack-batteries-included-preprocessor": "^2.4.1",
        "@cypress/webpack-preprocessor": "^5.17.1",
        "@types/lodash": "^4.14.196",
        "@types/node": "^18.16.1",
        "@types/webpack": "^5.28.1",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint": "^8.45.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-plugin-chai-expect": "^3.0.0",
        "eslint-plugin-cypress": "^2.13.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-watch": "^8.0.0",
        "husky": "^8.0.3",
        "jscpd": "^3.5.9",
        "lint-staged": "^13.2.3",
        "madge": "^6.1.0",
        "stop-only": "^3.3.1",
        "ts-loader": "^9.4.4",
        "typescript": "~5.0.4",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "yarpm": "^1.2.0"
    },
    "peerDependencies": {
        "@types/readable-stream": "^2.3.15"
    },
    "overrides": {
        "eslint": "$eslint"
    }
}